        <%  for(let i=0;i<answers.length;i++)  { %>
          <p>Meal Type:- <%= answers[i].mealType%></p>
          <p>Food Type:-</p>
          <ul>
            <% for(let q=0;q<answers[i].foodType.length;q++) {%>
